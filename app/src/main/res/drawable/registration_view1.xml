<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="rectangle">
            <corners android:radius="60px" />
            <solid
                android:color="#3394eff4" />
        </shape>
    </item>
    <item android:bottom="1dp">
        <shape
            android:shape="rectangle">
            <corners android:radius="60px" />
            <solid
                android:color="#3394eff4" />
        </shape>
    </item>
    <item  android:right="1dp" android:left="1dp" android:bottom="3dp">
        <shape
            android:shape="rectangle">
            <corners android:radius="60px" />
            <solid
                android:color="#3394eff4" />
        </shape>
    </item>
    <item android:right="2dp" android:left="2dp" android:bottom="5dp">
        <shape
            android:shape="rectangle">
            <stroke android:color="#00ffffff"
                android:width="1dp"/>
            <solid android:color="#00ffffff"/>
            <corners android:radius="60px"/>
        </shape>
    </item>
</layer-list>